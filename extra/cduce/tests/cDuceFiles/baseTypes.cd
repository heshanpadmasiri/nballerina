type BNull = `null
type BString = String
type BInt = (`int, Int, `pos | `neg)
type BBoolean = Bool
type BError = (`error, Any)
type BTop = BAny | BError
type BList = [BTop*]
type BAny = BNull | BString | BInt | BBoolean | BList
type BByte = (`int, 0--255, `pos)
type BFloat = (`float, Int, `pos | `neg)
type BDecimal = (`decimal, (Int, Int))

type T1 = BAny
type T2 = BBoolean
type T3 = BByte
type T4 = BInt
type T5 = BFloat
type T6 = BString
type T7 = BNull
type T8 = BNull
type T9 = BError
type T11 = BDecimal
