type BInt = (`int, Int, `pos | `neg)
type BBoolean = Bool
type BDecimal = (`decimal, (Int, Int))
type BFloat = (`float, Int, `pos | `neg)
type BList = [BTop*]
type BNull = `null
type BString = String
type BAny = BBoolean | BDecimal | BFloat | BInt | BList | BNull | BString
type BError = (`error, Any)
type BTop = BAny | BError

type notInt = BTop\BInt
type newInt = BInt | BNull & BTop\BNull
type myInt = BInt
